{
  "name": "contract_api",
  "version": "1.0.0",
  "description": "REST API for contract generation based in Colombian law",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "npx rimraf dist && npx tsc",
    "preserve": "npm run build",
    "serve": "npx concurrently 'npx tsc -w' 'nodemon dist/src/index.js'",
    "prestart": "npm run build",
    "start": "node ./dist/src/index.js",
    "test": "test"
  },
  "keywords": [
    "REST",
    "API",
    "Typescript",
    "Nodejs",
    "KOAjs"
  ],
  "author": "Cristian Morales",
  "license": "ISC",
  "dependencies": {
    "@types/koa": "2.13.6",
    "concurrently": "8.2.0",
    "fast-safe-stringify": "2.1.1",
    "google-auth-library": "9.0.0",
    "googleapis": "123.0.0",
    "koa": "2.14.2",
    "koa-parser": "1.0.8",
    "koa-router": "12.0.0",
    "megajs": "1.1.3",
    "node-fetch": "3.3.2",
    "rimraf": "5.0.1",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@types/koa-router": "7.4.4",
    "@types/node": "20.4.1",
    "nodemon": "3.0.1",
    "typescript": "5.1.6"
  }
}
